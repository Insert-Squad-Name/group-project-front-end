{{#each entries}}
<div class="col-xs-6 col-lg-4 page-entries" >
    <button type="button" class="close close-page-button" data-dismiss="alert" aria-label="Close" data-delete-page="{{_id}}"><span data-delete-page="{{_id}}" aria-hidden="true">&times;</span></button>
    <p>{{{body}}}</p><br>
    <p><a class="btn btn-default update-entry" href="#" role="button" data-page-id="{{_id}}">Update Entry</a></p>
</div>
{{/each}}
